{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Schema for club activities ",
    "descriptions": "The root schema comprises the entire JSON document.",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "date": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "location": {
            "type": "string",
            "title": "The location schema"
        }
    },
    "required": [
        "name",
        "date",
        "location"
    ],
    "additionalProperties": false
}